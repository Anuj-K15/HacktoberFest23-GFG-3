#include<iostream>
using namespace std;
int main()
{
    int num;
    cout<<"enter the number of array"<<endl;
    cin>>num;
    int arr[num];
    cout << "enter element of array: ";
    for (int i =0;i < num;i++) {
        cin >> arr[i];
    }
    int small = arr[0];
    for (int i = 1; i < num; i++) {
        if (arr[i] < small) {
            small = arr[i];
        }
    }
    cout << "smallest number in array is  " << small << endl;
 return 0;
}
